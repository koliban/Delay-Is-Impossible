<manifest
    package="com.thefat.lingjunqi.getup"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_mine"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".Activity.AlarmActivity"
            android:label="@string/alarm_activity_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".Activity.AlarmPreferencesActivity"
            android:label="@string/alarm_preferences_activity_name">

        </activity>

    </application>

</manifest>
